<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title class="titre">{% block title %}Wild Spirit{% endblock %}</title>
        <link rel="icon" type="image/png" href="{{ asset('build/images/logos/logo_head.png') }}" />
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block header %}{% endblock header %}
        {% block body %}{% endblock body %}
        {% block footer %}{% endblock footer %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock javascripts %}
    </body>
</html>
